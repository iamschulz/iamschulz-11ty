<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<title>
    {% if title.length %} {{ title }} &bull; {% endif %}
    {{ baseData.pageTitle }}
</title>
<link rel="preload" href="{{ '/style.css' | url }}" as="style">
<link rel="preload" href="{{ '/main.js' | url }}" as="script">
<link rel="preload" href="{{ '/assets/BlankRiver.woff2' | url }}" as="font" crossorigin>
<link rel="stylesheet" href="{{ '/style.css' | url }}">
<link rel="icon" href="/assets/favicon/favicon.svg">
<link rel="apple-touch-icon" sizes="180x180" href="{{ '/assets/favicon/apple-touch-icon.png' | url }}">
<link rel="icon" type="image/png" sizes="32x32" href="{{ '/assets/favicon/favicon-32x32.png' | url }}">
<link rel="icon" type="image/png" sizes="16x16" href="{{ '/assets/favicon/favicon-16x16.png' | url }}">
<link rel="manifest" href="{{ '/manifest.json' | url }}">
<link rel="alternate" type="application/rss+xml" title="RSS" href="{{ '/index.xml' | absoluteUrl(baseData.url) }}">
<link rel="webmention" href="https://webmention.io/iamschulz.com/webmention" />
<link rel="pingback" href="https://webmention.io/iamschulz.com/xmlrpc">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<meta name="description" content="{{ baseData.meta.description }}">
<meta name="author" content="{{ baseData.meta.authorName }}">
<meta name="theme-color" content="#0E181B">
<meta name="google-site-verification" content="{{ baseData.meta.googleSiteVerification }}">
<link href="mailto:hallo@iamschulz.de" rel="me">
